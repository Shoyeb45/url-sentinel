{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/favicon_io/(.*)", "dest": "/favicon_io/$1" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*\\.js)$", "dest": "/$1" },
    { "src": "/(.*\\.css)$", "dest": "/$1" },
    { "src": "/(.*\\.webmanifest)$", "dest": "/$1" },
    { "src": "/(.*\\.(png|jpg|jpeg|svg|ico|json))$", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
